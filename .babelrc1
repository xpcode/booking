{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "plugins": [
    "antd", ["import", {
      "style": true,
      "libraryName": "antd-mobile"
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}
